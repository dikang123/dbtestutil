{
  "name": "dbtestutil",
  "version": "3.0.2",
  "description": "Launches an isolated instance of MySQL",
  "main": "DbTestUtil.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "jshint *.js ; cp -n test/db.conf.example test/db.conf || true",
    "test": "mocha -R spec test/*.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ssimicro/dbtestutil"
  },
  "author": "SSi Micro Ltd <propellorheads@ssimicro.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ssimicro/dbtestutil/issues"
  },
  "homepage": "https://github.com/ssimicro/dbtestutil",
  "dependencies": {
    "fmtr": "^1.0.3",
    "lodash": "^4.13.1",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.5.2",
    "ssi-logger": "^2.5.0"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.9.2",
    "mocha": "^2.5.3",
    "mysql": "^2.11.1",
    "nconf": "^0.8.4"
  },
  "jshintConfig": {
    "bitwise": true,
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "undef": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false
    }
  }
}
